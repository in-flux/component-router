{
  "name": "in-flux",
  "version": "1.0.0",
  "description": "Flux-based partial routing solution",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:lib && npm run build:example",
    "build:clean": "rm -rf lib example",
    "build:lib": "babel -sd lib src",
    "build:example": "webpack",
    "prepublish": "npm run build",
    "start": "NODE_ENV='development' webpack-dev-server --history-api-fallback",
    "lint": "eslint *.js src test",
    "karma": "karma start ./karma.conf.js",
    "test": "npm run karma --  --single-run --reporters nyan",
    "test:dev": "npm run karma -- --no-single-run --auto-watch --reporters nyan",
    "test:cov:clean": "rm -rf ./coverage && mkdir -p ./coverage",
    "test:cov": "npm run test:cov:clean && npm run karma -- --single-run --reporters coverage",
    "test:coveralls": "npm run test:cov && cat ./coverage/phantomjs/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "check": "npm run lint && npm test && npm run test:coveralls && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nkbt/in-flux.git"
  },
  "keywords": [
    "component",
    "react-component",
    "flux-component",
    "react",
    "in-flux",
    "router",
    "url-router",
    "url-state",
    "partial routing",
    "partial"
  ],
  "author": "Nik Butenko <nik@butenko.me> (http://butenko.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkbt/in-flux/issues"
  },
  "homepage": "https://github.com/nkbt/in-flux",
  "dependencies": {
    "classnames": "^2.1.2",
    "flux": "^2.0.3",
    "flux-common-store": "^1.0.1",
    "lodash": "^3.9.3",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-loader": "^5.1.4",
    "babel-plugin-object-assign": "^1.1.0",
    "coveralls": "^2.11.2",
    "css-loader": "^0.15.1",
    "eslint": "^0.23.0",
    "eslint-loader": "^0.14.0",
    "eslint-plugin-react": "^2.5.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.5.2",
    "inject-loader": "^2.0.0",
    "isparta-loader": "^0.2.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.36",
    "karma-chrome-launcher": "^0.1.12",
    "karma-coverage": "^0.4.2",
    "karma-jasmine": "^0.3.5",
    "karma-nyan-reporter": "0.0.60",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-webpack": "^1.5.1",
    "node-libs-browser": "^0.5.2",
    "phantomjs": "^1.9.17",
    "react-hot-loader": "^1.2.7",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
